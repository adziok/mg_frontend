{
    "root": "build/",
    "canonical_host": "${HOST}",
    "encoding": "UTF-8",
    "clean_urls": false,
    "routes": {
        "/**": "index.html"
    },
    "proxies": {
        "/api/": {
            "origin": "${API_URL_HTTP}"
        }
    },
    "headers": {
        "/**": {
            "Access-Control-Allow-Origin": "*"
        }
    }
}